# platform :ios, '9.0'

def pods_list
    pod 'Siesta'
end

target 'WordsCounter' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  pods_list

  # Pods for WordsCounter

  target 'WordsCounterTests' do
    inherit! :search_paths
  pods_list
  end

end 